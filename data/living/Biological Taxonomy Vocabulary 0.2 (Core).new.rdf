        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdf-schema/#ch_label"/>
    </rdf:Description>
    <owl:AnnotationProperty rdf:about="&biol;order">
        <rdfs:label xml:lang="en">order</rdfs:label>
        <rdfs:label xml:lang="zh-cn">目</rdfs:label>
        <dcterms:description xml:lang="en">The fourth highest level of classification. For example &quot;Primates&quot;.</dcterms:description>
        <dcterms:description xml:lang="zh-cn"></dcterms:description>
        <rdfs:domain rdf:resource="&biol;Taxonomy"/>
        <rdfs:subPropertyOf rdf:resource="&biol;rank"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_order"/>
        <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_order"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&biol;name">
        <rdfs:label xml:lang="en">name</rdfs:label>
        <rdfs:label xml:lang="zh-cn">名称</rdfs:label>
        <dcterms:description rdf:datatype="&rdf;XMLLiteral">
      
    </dcterms:description>
        <dcterms:description rdf:datatype="&rdf;XMLLiteral">
      A scientific name; a binomial or trinomial. For example, &lt;i class=&quot;taxon&quot;&gt;Homo sapiens&lt;/i&gt;.
    </dcterms:description>
        <rdfs:domain rdf:resource="&biol;Taxonomy"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_name"/>
        <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_name"/>
    </owl:AnnotationProperty>
    <rdf:Description rdf:about="&rdf;type">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdf-schema/#ch_type"/>
    </rdf:Description>
    <owl:AnnotationProperty rdf:about="&biol;class">
        <rdfs:label xml:lang="en">class</rdfs:label>
        <rdfs:label xml:lang="zh-cn">纲</rdfs:label>
        <dcterms:description rdf:datatype="&rdf;XMLLiteral">
    </dcterms:description>
        <dcterms:description rdf:datatype="&rdf;XMLLiteral">
      The third highest level of classification. For example &lt;i class=&quot;taxon&quot;&gt;Mammalia&lt;/i&gt;.
    </dcterms:description>
        <rdfs:domain rdf:resource="&biol;Taxonomy"/>
        <rdfs:subPropertyOf rdf:resource="&biol;rank"/>
        <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_class"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_class"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&biol;species">
        <rdfs:label xml:lang="en">species</rdfs:label>
        <rdfs:label xml:lang="zh-cn">种</rdfs:label>
        <dcterms:description rdf:datatype="&rdf;XMLLiteral">
      The lowest level of classification supported by this vocabulary. For example &lt;i class=&quot;taxon&quot;&gt;Sapiens&lt;/i&gt;. To include information more specific than species, use &lt;a href=&quot;#term_rank&quot;&gt;biol:rank&lt;/a&gt; or &lt;a href=&quot;#section_Abstract&quot;&gt;a more specialist vocabulary&lt;/a&gt;.
    </dcterms:description>
        <dcterms:description rdf:datatype="&rdf;XMLLiteral">
      该词汇集支持的最低分类。
    </dcterms:description>
        <rdfs:domain rdf:resource="&biol;Taxonomy"/>
        <rdfs:subPropertyOf rdf:resource="&biol;rank"/>
        <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_species"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_species"/>
    </owl:AnnotationProperty>
