  <!-- http://online-presence.net/opo/ns#Action , added in 0.4-->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#Action">
    <rdfs:label xml:lang="en">Action</rdfs:label>
    <rdfs:label xml:lang="zh-CN">行为</rdfs:label>
    <rdfs:subClassOf rdf:resource="&event;Event" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#SourceOfPublishing" />

    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      An action currently performed by the Agent (e.g., listening to music, reading a book).
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">由[代理]做出的行为（比如听音乐，读书等等）。</rdfs:comment>
  </owl:Class>




  <!-- http://online-presence.net/opo/ns#Activity -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#Activity">
    <rdfs:label xml:lang="en">Activity</rdfs:label>
    <rdfs:label xml:lang="zh-CN">活跃</rdfs:label>
    <rdfs:subClassOf
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatusComponent" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Contactability" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Disturbability" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Visibility" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      OnlineStatusComponent used to represent the state of
      Activity of the Agent (e.g., Active, Inactive,
      ProlongedInactive).
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">该[在线状态要素]用来表示[代理]的[活跃]状态。</rdfs:comment>
  </owl:Class>



  <!-- http://online-presence.net/opo/ns#Contactability -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#Contactability">
    <rdfs:label xml:lang="en">Contactability</rdfs:label>
    <rdfs:label xml:lang="zh-CN">接触</rdfs:label>
    <rdfs:subClassOf
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatusComponent" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Disturbability" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Visibility" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Activity" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      OnlineStatusComponent used to represent the state of
      contactability of an Agent. Contatctability relates to the
      possiblity of other Agents to conatct the Agent freely or
      only with regard to some conditions.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">该[在线状态要素]用于表示[代理]的接触性的状态。[接触性]涉及了其他[代理]自由地或就某些条件联系到该[代理]的可能性。</rdfs:comment>
  </owl:Class>



  <!-- http://online-presence.net/opo/ns#Disturbability -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#Disturbability">
    <rdfs:label xml:lang="en">Disturbability</rdfs:label>
    <rdfs:label xml:lang="zh-CN">打扰</rdfs:label>
    <rdfs:subClassOf
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatusComponent" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Visibility" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Activity" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Contactability" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      OnlineStatusComponent used to represent the wish of an Agent
      to be or not to be disturbed.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">该[在线状态要素]用于表示[代理]允许或不想被打扰的意愿。</rdfs:comment>
  </owl:Class>



  <!-- http://online-presence.net/opo/ns#Findability -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#Findability">
    <rdfs:label xml:lang="en">Findability</rdfs:label>
    <rdfs:label xml:lang="zh-CN">查找</rdfs:label>
    <rdfs:subClassOf
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresenceComponent" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Notifiability" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatus" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The OnlinePresenceComponent used to represent the
      possibility to find contact details of an Agent in a public
      listing
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">该[在线状况要素]用于表示在公共列表中找到联络信息的可能性。</rdfs:comment>
  </owl:Class>



  <!-- http://online-presence.net/opo/ns#Notifiability -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#Notifiability">
    <rdfs:label xml:lang="en">Notifiability</rdfs:label>
    <rdfs:label xml:lang="zh-CN">提醒</rdfs:label>
    <rdfs:subClassOf
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresenceComponent" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatus" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Findability" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The OnlinePresenceComponent used to represent the attitude
      of an Agent towards interaction with applications i.e., the
      possibility of applications to show notifications.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">该[在线状况要素]用于表示[代理]面对应用交互的态度，比如，应用显示通知/提醒的可能性。</rdfs:comment>
  </owl:Class>



  <!-- http://online-presence.net/opo/ns#OnlinePresence -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#OnlinePresence">
    <rdfs:label xml:lang="en">OnlinePresence</rdfs:label>
    <rdfs:label xml:lang="zh-CN">在线情况</rdfs:label>
    <rdfs:subClassOf rdf:resource="&owl;Thing" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty
					rdf:resource="http://online-presence.net/opo/ns#hasStatusComponent" />
        <owl:onClass
					rdf:resource="http://online-presence.net/opo/ns#Findability" />
        <owl:maxCardinality
					rdf:datatype="&xsd;nonNegativeInteger">
          1
        </owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty
					rdf:resource="http://online-presence.net/opo/ns#hasStatusComponent" />
        <owl:onClass
					rdf:resource="http://online-presence.net/opo/ns#Notifiability" />
        <owl:maxCardinality
					rdf:datatype="&xsd;nonNegativeInteger">
          1
        </owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty
					rdf:resource="http://online-presence.net/opo/ns#hasStatusComponent" />
        <owl:onClass
					rdf:resource="http://online-presence.net/opo/ns#OnlineStatus" />
        <owl:maxCardinality
					rdf:datatype="&xsd;nonNegativeInteger">
          1
        </owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresenceComponent" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Action" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#SourceOfPublishing" />

    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      OnlinePresence, described in termes of various
      OnlinePresenceComponents to represent the attitude of an
      Agent towards interaction with other Agents and
      Applications.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[在线状况]根据不同[在线状况要素]进行描述，表示了一个[代理]面对与其他[代理]和[应用]的交互的态度。</rdfs:comment>
  </owl:Class>



  <!-- http://online-presence.net/opo/ns#OnlinePresenceComponent -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#OnlinePresenceComponent">
    <rdfs:label xml:lang="en">OnlinePresenceComponent</rdfs:label>
    <rdfs:label xml:lang="zh-CN">在线情况要素</rdfs:label>
    <rdfs:subClassOf rdf:resource="&owl;Thing" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:comment xml:lang="en">
      A component of OnlinePresence used to represent one of its
      dimensions.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[在线状况]的一个要素，用于表示它的一个维度。</rdfs:comment>
  </owl:Class>

  <!-- http://online-presence.net/opo/ns#OnlineStatus -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#OnlineStatus">
    <rdfs:label xml:lang="en">OnlineStatus</rdfs:label>
    <rdfs:label xml:lang="zh-CN">在线状态</rdfs:label>
    <rdfs:subClassOf
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresenceComponent" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty
					rdf:resource="http://online-presence.net/opo/ns#hasStatusComponent" />
        <owl:onClass
					rdf:resource="http://online-presence.net/opo/ns#Notifiability" />
        <owl:maxCardinality
					rdf:datatype="&xsd;nonNegativeInteger">
          1
        </owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty
					rdf:resource="http://online-presence.net/opo/ns#hasStatusComponent" />
        <owl:onClass
					rdf:resource="http://online-presence.net/opo/ns#Visibility" />
        <owl:maxCardinality
					rdf:datatype="&xsd;nonNegativeInteger">
          1
        </owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty
					rdf:resource="http://online-presence.net/opo/ns#hasStatusComponent" />
        <owl:onClass
					rdf:resource="http://online-presence.net/opo/ns#Activity" />
        <owl:maxCardinality
					rdf:datatype="&xsd;nonNegativeInteger">
          1
        </owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty
					rdf:resource="http://online-presence.net/opo/ns#hasStatusComponent" />
        <owl:onClass
					rdf:resource="http://online-presence.net/opo/ns#Disturbability" />
        <owl:maxCardinality
					rdf:datatype="&xsd;nonNegativeInteger">
          1
        </owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatusComponent" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Findability" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Notifiability" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The OnlinePresenceComponent used to represent the attitude
      of an Agent towards the possibility of communication with
      other Agents. The OnlineStatus is defined by its components.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[在线状况要素]用于表示一个[代理]面对和其他[代理]的可能沟通时，这个[代理]的态度。[在线状态]由它的组成要素定义。</rdfs:comment>
  </owl:Class>



  <!-- http://online-presence.net/opo/ns#OnlineStatusComponent -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#OnlineStatusComponent">
    <rdfs:label xml:lang="en">OnlineStatusComponent</rdfs:label>
    <rdfs:label xml:lang="zh-CN">在线状态要素</rdfs:label>
    <rdfs:subClassOf rdf:resource="&owl;Thing" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatus" />
    <rdfs:comment xml:lang="en">
      The component of the OnlineStatus representing one of its
      dimensions.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[在线状态]的要素表示了它的一个维度。</rdfs:comment>
  </owl:Class>

  <!-- http://online-presence.net/opo/ns#SourceOfPublishing , added in 0.4-->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#SourceOfPublishing">
