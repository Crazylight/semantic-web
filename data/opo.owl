  <!-- http://online-presence.net/opo/ns#Active -->

  <Activity rdf:about="http://online-presence.net/opo/ns#Active">
    <rdfs:label xml:lang="en">Active</rdfs:label>
    <rdfs:label xml:lang="zh-CN">活跃的</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent is active on the system.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]在系统上是活跃的。</rdfs:comment>
  </Activity>



  <!-- http://online-presence.net/opo/ns#AllNotificationsPass -->

  <Notifiability
		rdf:about="http://online-presence.net/opo/ns#AllNotificationsPass">
    <rdfs:label xml:lang="en">AllNotificationsPass</rdfs:label>
    <rdfs:label xml:lang="zh-CN">接收所有通知</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      Agent can receive notifications from applications
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]能够接收来自应用的通知。</rdfs:comment>
  </Notifiability>



  <!-- http://online-presence.net/opo/ns#Available -->

  <Disturbability
		rdf:about="http://online-presence.net/opo/ns#Available">
    <rdfs:label xml:lang="en">Available</rdfs:label>
    <rdfs:label xml:lang="zh-CN">有空的</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The agent is not busy and therefore available for contatc by
      other Agents.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]不忙，因此可以被其他[代理]联系。</rdfs:comment>
  </Disturbability>



  <!-- http://online-presence.net/opo/ns#ConstrainedContactability -->

  <Contactability
		rdf:about="http://online-presence.net/opo/ns#ConstrainedContactability">
    <rdfs:label xml:lang="en">ConstrainedContactability</rdfs:label>
    <rdfs:label xml:lang="zh-CN">可联络性限制</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent cannot be contated by anyone on the Web, but the
      contactability is controled by some rules/policies.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]无法被网络上的任何人联络到，但是可联络性由一些规则/策略控制。</rdfs:comment>
  </Contactability>



  <!-- http://online-presence.net/opo/ns#ConstrainedFindability -->

  <Findability
		rdf:about="http://online-presence.net/opo/ns#ConstrainedFindability">
    <rdfs:label xml:lang="en">ConstrainedFindability</rdfs:label>
    <rdfs:label xml:lang="zh-CN">被查找限制</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent&#39;s contact details cannot be found by anyone on
      the Web, but the findability is controled by some
      rules/policies.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]无法被网络上的任何人找到联系信息，但是可查找性由一些规则/策略控制。</rdfs:comment>
  </Findability>



  <!-- http://online-presence.net/opo/ns#DoNotDisturb -->

  <Disturbability
		rdf:about="http://online-presence.net/opo/ns#DoNotDisturb">
    <rdfs:label xml:lang="en">DoNotDisturb</rdfs:label>
    <rdfs:label xml:lang="zh-CN">禁止打扰</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent is busy and does not want to be contacted by other
      Agents.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]很忙，不想被其它[代理]联系到。</rdfs:comment>
  </Disturbability>



  <!-- http://online-presence.net/opo/ns#FreelyContactable -->

  <Contactability
		rdf:about="http://online-presence.net/opo/ns#FreelyContactable">
    <rdfs:label xml:lang="en">FreelyContactable</rdfs:label>
    <rdfs:label xml:lang="zh-CN">自由联络</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent can be contacted by anyone on the Web
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]能够被网络上的任何人联络到。</rdfs:comment>
  </Contactability>



  <!-- http://online-presence.net/opo/ns#Inactive -->

  <Activity
		rdf:about="http://online-presence.net/opo/ns#Inactive">
    <rdfs:label xml:lang="en">Inactive</rdfs:label>
    <rdfs:label xml:lang="zh-CN">不活跃的</rdfs:label>
    <inactivityPeriod rdf:datatype="&xsd;duration">PT5M</inactivityPeriod>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent is inactive for at least 5 min.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]的不活跃时间在至少五分钟以内。</rdfs:comment>
  </Activity>



  <!-- http://online-presence.net/opo/ns#Invisible -->

  <Visibility
		rdf:about="http://online-presence.net/opo/ns#Invisible">
    <rdfs:label xml:lang="en">Invisible</rdfs:label>
    <rdfs:label xml:lang="zh-CN">不可见</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent&#39;s OnlineStatus is not visible to other Agents
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]的[在线状态]对于其他[代理]不可见。</rdfs:comment>
  </Visibility>



  <!-- http://online-presence.net/opo/ns#NotificationsConstrained -->

  <Notifiability
		rdf:about="http://online-presence.net/opo/ns#NotificationsConstrained">
    <rdfs:label xml:lang="en">NotificationsConstrained</rdfs:label>
    <rdfs:label xml:lang="zh-CN">通知限制</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The acceptance of notifications from applications is somehow
      constrained (by using some rules or policies).
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">以某种方式（通过使用一些规则或策略） 对接受来自应用程序的通知进行约束。</rdfs:comment>
  </Notifiability>



  <!-- http://online-presence.net/opo/ns#NotificationsProhibited -->

  <Notifiability
		rdf:about="http://online-presence.net/opo/ns#NotificationsProhibited">
    <rdfs:label xml:lang="en">NotificationsProhibited</rdfs:label>
    <rdfs:label xml:lang="zh-CN">禁止通知</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      Agent cannot receive any notification form applications
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]无法接收来自应用的通知。</rdfs:comment>
  </Notifiability>



  <!-- http://online-presence.net/opo/ns#ProlongedInactive -->

  <Activity
		rdf:about="http://online-presence.net/opo/ns#ProlongedInactive">
    <rdfs:label xml:lang="en">ProlongedInactive</rdfs:label>
    <rdfs:label xml:lang="zh-CN">长时间不活跃的</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <inactivityPeriod rdf:datatype="&xsd;duration">PT20M</inactivityPeriod>
    <rdfs:comment xml:lang="en">
      The Agent is inactive for at least 20 min.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]的不活跃时间超过二十分钟。</rdfs:comment>
  </Activity>



  <!-- http://online-presence.net/opo/ns#PubliclyFindable -->

  <Findability
		rdf:about="http://online-presence.net/opo/ns#PubliclyFindable">
    <rdfs:label xml:lang="en">PubliclyFindable</rdfs:label>
    <rdfs:label xml:lang="zh-CN">公开可查找</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent&#39;s contact details canbe found by anyone on the
      Web
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]的联系信息可被网络上的任何人找到。</rdfs:comment>
  </Findability>



  <!-- http://online-presence.net/opo/ns#Visible -->

  <Visibility
		rdf:about="http://online-presence.net/opo/ns#Visible">
    <rdfs:label xml:lang="en">Visible</rdfs:label>
    <rdfs:label xml:lang="zh-CN">可见</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent&#39;s OnlineStatus is visible to other Agents
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]的[在线状态]对于其他[代理]可见。</rdfs:comment>
  </Visibility>



