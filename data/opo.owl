			rdf:resource="http://online-presence.net/opo/ns#Action" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The source that can publish Online Presence data.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">能够发布[在线状况]数据的源。</rdfs:comment>
  </owl:Class>


  <!-- http://online-presence.net/opo/ns#Visibility -->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#Visibility">
    <rdfs:label xml:lang="en">Visibility</rdfs:label>
    <rdfs:label xml:lang="zh-CN"></rdfs:label>
    <rdfs:subClassOf
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatusComponent" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Disturbability" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Activity" />
    <owl:disjointWith
			rdf:resource="http://online-presence.net/opo/ns#Contactability" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      OnlineStatusComponent used to represent the possiblity of
