>

  <owl:Ontology rdf:about="http://online-presence.net/opo/ns#">
    <dc:date rdf:datatype="&xsd;date">2010-07-01</dc:date>
    <dc:creator rdf:datatype="&xsd;string">Milan Stankovic</dc:creator>
    <owl:versionInfo>Revision 0.5.2</owl:versionInfo>
    <dc:contributor>Jelena Jovanovic</dc:contributor>
    <dc:contributor>Alexandre Passant</dc:contributor>
    <dc:title xml:lang="en">OnlinePresence Ontology</dc:title>
    <dc:title xml:lang="zh-CN">[在线状况]本体</dc:title>
    <dc:description xml:lang="en">
      Ontology for representing OnlinePresence.
    </dc:description>
    <dc:description xml:lang="zh-CN">用于描述[在线状况]的本体。</dc:description>
    <rdfs:seeAlso
			rdf:resource="http://online-presence.net/opo/spec/" />
  </owl:Ontology>

  <rdf:Description rdf:about="http://rdfs.org/sioc/ns#Item">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://rdfs.org/sioc/ns#UserAccount">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Image">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://rdfs.org/sioc/ns#has_creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://online-presence.net/opo/ns#">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/NET/c4dm/event.owl#Event">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Group">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://data.semanticweb.org/ns/swc/ontology#OrganisedEvent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://online-presence.net/opo/ns#DoNotDistrub">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Document">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

  <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

  <owl:AnnotationProperty rdf:about="&dc;date"/>
  <owl:AnnotationProperty rdf:about="&dc;title"/>
  <owl:AnnotationProperty rdf:about="&dc;description"/>
  <owl:AnnotationProperty rdf:about="&dc;contributor"/>
  <owl:AnnotationProperty rdf:about="&dc;creator"/>

  <!-- 
		///////////////////////////////////////////////////////////////////////////////////////
		//
		// Object properties
		//
		///////////////////////////////////////////////////////////////////////////////////////
	-->

  <!-- http://online-presence.net/opo/ns#isPresenceComponentOf -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#isPresenceComponentOf">
    <rdfs:label xml:lang="en">isPresenceComponentOf</rdfs:label>
    <rdfs:label xml:lang="zh-CN">是……的[在线状况要素]</rdfs:label>
    <rdfs:comment xml:lang="en">
      The OnlinePresence that has this OnlinePresenceComponent
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">含有这个[在线状况要素]的[在线状况]。</rdfs:comment>
    <rdfs:range rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresenceComponent" />
    <owl:inverseOf
          rdf:resource="http://online-presence.net/opo/ns#hasPresenceComponent" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
  </owl:ObjectProperty>


  <!-- http://online-presence.net/opo/ns#avatar -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#avatar">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
    <rdfs:label xml:lang="en">avatar</rdfs:label>
    <rdfs:label xml:lang="zh-CN">头像</rdfs:label>
    <rdfs:comment xml:lang="en">
      The graphical representation of the Agent used to depict him
      in online systems.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">用来在在线系统描绘[代理]的图形化表示。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:range rdf:resource="&foaf;Image" />
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#currentLocation , added in 0.4-->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#currentLocation">
    <rdfs:label xml:lang="en">currentLocation</rdfs:label>
    <rdfs:label xml:lang="zh-CN">当前位置</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      Current location of the Agent
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]的当前位置。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:range rdf:resource="&wgs84;SpatialThing" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#currentAction , added in 0.4-->

  <owl:ObjectProperty
  rdf:about="http://online-presence.net/opo/ns#currentAction">
    <rdfs:label xml:lang="en">currentAction</rdfs:label>
    <rdfs:label xml:lang="zh-CN">当前行为</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      Current action performed by the Agent
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">当前[代理]所作出的行为。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:range rdf:resource="http://online-presence.net/opo/ns#Action" />
  </owl:ObjectProperty>


  <!-- http://online-presence.net/opo/ns#declaredBy -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#declaredBy">
    <rdfs:label xml:lang="en">declaredBy</rdfs:label>
    <rdfs:label xml:lang="zh-CN">由……宣示</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The Agent that declared the OnlinePresence
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]宣示的[在线状况]。</rdfs:comment>
    <owl:inverseOf
			rdf:resource="http://online-presence.net/opo/ns#declaresOnlinePresence" />
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:range rdf:resource="&foaf;Agent" />
  </owl:ObjectProperty>



  <!-- http://online-presence.net/opo/ns#declaredOn -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#declaredOn">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
    <rdfs:label xml:lang="en">declaredOn</rdfs:label>
    <rdfs:label xml:lang="zh-CN">宣示人</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The sioc:UserAccount account where the OnlinePresence was declared.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">宣示[在线状况]的sioc:UserAccount账户。</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="&sioc;has_creator"/>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:range rdf:resource="&sioc;UserAccount" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#publishedFrom -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#publishedFrom">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
    <rdfs:label xml:lang="en">publishedFrom</rdfs:label>
    <rdfs:label xml:lang="zh-CN">发布于</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The source from which the OnlinePresence is declared (e.g., a mobile device)
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">宣示[在线状况]的源（比如，一个手机设备）。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:range rdf:resource="http://online-presence.net/opo/ns#SourceOfPublishing" />
  </owl:ObjectProperty>


  <!-- http://online-presence.net/opo/ns#declaresOnlinePresence -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#declaresOnlinePresence">
    <rdfs:label xml:lang="en">declaresOnlinePresence</rdfs:label>
    <rdfs:label xml:lang="zh-CN">宣示</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The OnlinePresence declared by the Agent
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">[代理]所宣示的[在线状况]。</rdfs:comment>
    <rdfs:range	rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <owl:inverseOf
			rdf:resource="http://online-presence.net/opo/ns#declaredBy" />
    <rdfs:domain rdf:resource="&foaf;Agent" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#hasPresenceComponent -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#hasPresenceComponent">
    <rdfs:label xml:lang="en">hasOnlinePresence</rdfs:label>
    <rdfs:label xml:lang="zh-CN">在线情况</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      OnlinePresenceComponent included in this OnlinePresence
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">这个[在线状况]所包括的[在线状况要素]。</rdfs:comment>
    <owl:inverseOf
			rdf:resource="http://online-presence.net/opo/ns#isPresenceComponentOf" />
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:range	rdf:resource="http://online-presence.net/opo/ns#OnlinePresenceComponent" />
  </owl:ObjectProperty>


  <!-- http://online-presence.net/opo/ns#hasStatusComponent -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#hasStatusComponent">
    <rdfs:label xml:lang="en">hasStatusComponent</rdfs:label>
    <rdfs:label xml:lang="zh-CN">状态要素</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      OnlineStatusComponent included in this OnlineStatus
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">这个[在线状态]所包括的[在线状态要素]。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatus" />
    <rdfs:range	rdf:resource="http://online-presence.net/opo/ns#OnlineStatusComponent" />
    <owl:inverseOf
			rdf:resource="http://online-presence.net/opo/ns#isStatusComponentOf" />
  </owl:ObjectProperty>



  <!-- http://online-presence.net/opo/ns#isStatusComponentOf -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#isStatusComponentOf">
    <rdfs:label xml:lang="en">isStatusComponentOf</rdfs:label>
    <rdfs:label xml:lang="zh-CN">是……的状态要素</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The OnlineStatus that includes this OnlineStatusComponent.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">这个[在线状态要素]所包括的[在线状态]。</rdfs:comment>
    <rdfs:range	rdf:resource="http://online-presence.net/opo/ns#OnlineStatus" />
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlineStatusComponent" />
    <owl:inverseOf
			rdf:resource="http://online-presence.net/opo/ns#hasStatusComponent" />

  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#sourceDescription -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#sourceDescription">
    <rdfs:label xml:lang="en">sourceDescription</rdfs:label>
    <rdfs:label xml:lang="zh-CN">来源描述</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      the description of the SourceOfPublishing.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">关于[发布源]的描述。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#SourceOfPublishing" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#customMessage -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#customMessage">
    <rdfs:label xml:lang="en">customMessage</rdfs:label>
    <rdfs:label xml:lang="zh-CN">自定义信息</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      A message associated with the OnlinePresence, often used in
      chat programs and social networks as custom title as well as in microblogging as status message.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">与该[在线状况]关联的信息，通常和微博中的状态信息一样作为自定义标题用于聊天程序和社交网络。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:range	rdf:resource="&sioc;Item" />

  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#intendedFor  added in 0.5 -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#intendedFor">
    <rdfs:label xml:lang="en">intendedFor</rdfs:label>
    <rdfs:label xml:lang="zh-CN">用于</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      A group of people belonging to a space for sharing online psresence data - the intended audience of presence information.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">属于一个在线状况数据（目标受众的状况）的空间的一组人。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#OnlinePresence" />
    <rdfs:range rdf:resource="http://online-presence.net/opo/ns#SharingSpace" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#commonInterest  added in 0.5 -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#commonInterest">
    <rdfs:label xml:lang="en">Common Interest</rdfs:label>
    <rdfs:label xml:lang="zh-CN">共同兴趣</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      An interst shared by people in the Sharing Space.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">在[共享空间]的人分享的兴趣。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#SharingSpace" />
    <rdfs:range rdf:resource="&foaf;Document" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#currentlyIn  added in 0.5 -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#currentlyIn">
    <rdfs:label xml:lang="en">Currently In</rdfs:label>
    <rdfs:label xml:lang="zh-CN">当前位置</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The current location of people in the Sharing Space.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">在[共享空间]的人的当前位置。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#SharingSpace" />
    <rdfs:range rdf:resource="&wgs84;SpatialThing" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#basedNear  added in 0.5 -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#basedNear">
    <rdfs:label xml:lang="en">Based Near</rdfs:label>
    <rdfs:label xml:lang="zh-CN">附近</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The location where members of the Sharing Space are based.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">在[共享空间]的成员的位置。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#SharingSpace" />
    <rdfs:range rdf:resource="&wgs84;SpatialThing" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#schoolHomepage  added in 0.5 -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#schoolHomepage">
    <rdfs:label xml:lang="en">School Homepage</rdfs:label>
    <rdfs:label xml:lang="zh-CN">学校主页</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The homepage of school attended by people in the Sharing Space.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">在[共享空间]参与的人的学校主页。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#SharingSpace" />
    <rdfs:range rdf:resource="&foaf;Document" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#workplaceHomepage  added in 0.5 -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#workplaceHomepage">
    <rdfs:label xml:lang="en">Workplace Homepage</rdfs:label>
    <rdfs:label xml:lang="zh-CN">工作主页</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The homepage of workplace of people in the Sharing Space.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">在[共享空间]参与的人的工作主页。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#SharingSpace" />
    <rdfs:range rdf:resource="&foaf;Document" />
  </owl:ObjectProperty>

  <!-- http://online-presence.net/opo/ns#attendedOrganisedEvent  added in 0.5 -->

  <owl:ObjectProperty
		rdf:about="http://online-presence.net/opo/ns#attendedOrganisedEvent">
    <rdfs:label xml:lang="en">Attended Organised Event</rdfs:label>
    <rdfs:label xml:lang="zh-CN">参与组织的事件</rdfs:label>
    <rdfs:isDefinedBy
			rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">
      The organised event attended by people in the Sharing Space.
    </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">在共享空间人们参与组织的事件。</rdfs:comment>
    <rdfs:domain
			rdf:resource="http://online-presence.net/opo/ns#SharingSpace" />
    <rdfs:range rdf:resource="&swc;OrganisedEvent" />
