    <rdfs:comment xml:lang="zh-CN">无法评论的状态信息。</rdfs:comment>
  </owl:Class>

  <!-- http://online-presence.net/opo/ns#SharingSpace , added in 0.4-->

  <owl:Class
		rdf:about="http://online-presence.net/opo/ns#SharingSpace">
    <rdfs:label xml:lang="en">SharingSpace</rdfs:label>
    <rdfs:label xml:lang="zh-CN">共享空间</rdfs:label>
    <rdfs:subClassOf rdf:resource="&foaf;Group" />
    <rdfs:isDefinedBy	rdf:resource="http://online-presence.net/opo/ns#" />
    <rdfs:comment xml:lang="en">A group of people belonging to a space for sharing online presence data - the intended audience of data.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">属于一个在线状况数据（目标受众的数据）的空间的一组人。</rdfs:comment>
  </owl:Class>


