  <owl:Class rdf:ID="PEPOrISAOrPortfolioTransferConfirmationV02">
    <owl:disjointWith>
      <owl:Class rdf:ID="PEPOrISAOrPortfolioTransferInstructionV02"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="RequestForPEPOrISAOrPortfolioInformationV01"/>
