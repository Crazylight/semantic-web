<?xml version="1.0"?>
<!DOCTYPE rdf:RDF[
  <!ENTITY lang 'http://purl.org/net/inkel/rdf/schemas/lang/1.1#'>
  <!ENTITY dc 'http://purl.org/dc/elements/1.1/'>
  <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
  <!ENTITY foaf 'http://xmlns.com/foaf/0.1/'>
  <!ENTITY this 'http://purl.org/net/inkel/rdf/schemas/lang/1.1'>
]>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:owl="http://www.w3.org/2002/07/owl#">
  <owl:Ontology rdf:about="http://purl.org/net/inkel/rdf/schemas/lang/1.1">
    <rdfs:label xml:lang="en">Speaks, Reads and Writes</rdfs:label>
    <rdfs:label xml:lang="es">Habla, Lee y Escribe</rdfs:label>
    <rdfs:label xml:lang="da">Taler, læser og skriver</rdfs:label>
    <rdfs:label xml:lang="zh-CN">说，读，写</rdfs:label>
    <rdfs:comment xml:lang="en">A vocabulary to express the languages a Person can speak, read or write</rdfs:comment>
    <rdfs:comment xml:lang="es">Un vocabulario para expresar los idiomas que una persona sabe hablar, leer o escribir</rdfs:comment>
    <rdfs:comment xml:lang="da">En samling gloser der kan bruges til at udtrykke hvilke sprog en person kan tale, læse eller skrive.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">用于表达一个人能说、能读或能写的语言的词汇集。</rdfs:comment>
  </owl:Ontology>
  <rdf:Property rdf:about="&lang;speaks">
    <rdfs:domain rdf:resource="&foaf;Person" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:subPropertyOf rdf:resource="&dc;language" />
    <rdfs:label xml:lang="en">Speaks Language</rdfs:label>
    <rdfs:label xml:lang="es">Habla Lenguaje</rdfs:label>
    <rdfs:label xml:lang="da">Taler sprog</rdfs:label>
    <rdfs:label xml:lang="zh-CN">说话语言</rdfs:label>
    <rdfs:comment xml:lang="en">The Person can speak in this language</rdfs:comment>
    <rdfs:comment xml:lang="es">La Persona puede hablar en este idioma</rdfs:comment>
    <rdfs:comment xml:lang="da">Personen kan tale dette sprog</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">某个人能够使用这种语言进行说话。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&this;" />
  </rdf:Property>
  <rdf:Property rdf:about="&lang;reads">
    <rdfs:domain rdf:resource="&foaf;Person" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:subPropertyOf rdf:resource="&dc;language" />
    <rdfs:label xml:lang="en">Reads Language</rdfs:label>
    <rdfs:label xml:lang="es">Lee Lenguaje</rdfs:label>
    <rdfs:label xml:lang="da">Læser sprog</rdfs:label>
    <rdfs:label xml:lang="zh-CN">阅读语言</rdfs:label>
    <rdfs:comment xml:lang="en">The Person can read in this language</rdfs:comment>
    <rdfs:comment xml:lang="es">La Persona puede leer en este idioma</rdfs:comment>
    <rdfs:comment xml:lang="da">Personen kan læse dette sprog</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">某个人能够使用这种语言进行阅读。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&this;" />
  </rdf:Property>
  <rdf:Property rdf:about="&lang;writes">
    <rdfs:domain rdf:resource="&foaf;Person" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:subPropertyOf rdf:resource="&dc;language" />
    <rdfs:label xml:lang="en">Writes Language</rdfs:label>
    <rdfs:label xml:lang="es">Escribe Lenguaje</rdfs:label>
    <rdfs:label xml:lang="da">Skriver sprog</rdfs:label>
    <rdfs:label xml:lang="zh-CN">写作语言</rdfs:label>
    <rdfs:comment xml:lang="en">The Person can write in this language</rdfs:comment>
    <rdfs:comment xml:lang="es">La Persona puede escribir en este lenguaje</rdfs:comment>
    <rdfs:comment xml:lang="da">Personen kan skrive dette sprog</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">某个人能够使用这种语言进行写作。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&this;" />
  </rdf:Property>
  <rdf:Property rdf:about="&lang;masters">
    <rdfs:domain rdf:resource="&foaf;Person" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:subPropertyOf rdf:resource="&lang;speaks" />
    <rdfs:subPropertyOf rdf:resource="&lang;reads" />
    <rdfs:subPropertyOf rdf:resource="&lang;writes" />
    <rdfs:label xml:lang="en">Speaks, Reads and Writes Language</rdfs:label>
    <rdfs:label xml:lang="es">Habla, Lee y Escribe Lenguaje</rdfs:label>
    <rdfs:label xml:lang="da">Taler, læser og skriver sprog</rdfs:label>
    <rdfs:label xml:lang="zh-CN">说，读，写</rdfs:label>
    <rdfs:comment xml:lang="en">The Person can Speak, Read and Write in this language</rdfs:comment>
    <rdfs:comment xml:lang="es">La Persona puede Hablar, Leer y Escribir en este idioma</rdfs:comment>
    <rdfs:comment xml:lang="da">Personen kan tale, læse og skrive dette sprog</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">某个人精通这种语言：能够说话，阅读和写作。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&this;" />
  </rdf:Property>
</rdf:RDF>