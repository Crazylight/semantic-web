  <rdf:Description rdf:about="http://purl.org/ontology/places">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <rdf:type rdf:resource="http://web.resource.org/cc/Work"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-11-27</dct:created>
    <dct:description xml:lang="en">The Places Ontology is a simple lightweight ontology for describing places of geographic interest.</dct:description>
    <dct:description xml:lang="zh-CN">地域本体是一个简单的轻量级本体，描述了与地理相关的地域。</dct:description>
    <dct:title xml:lang="en">The Places Ontology</dct:title>
    <dct:title xml:lang="zh-CN">地域本体</dct:title>
    <dct:type rdf:resource="http://purl.org/dc/dcmitype/Text"/>
    <vann:preferredNamespacePrefix>place</vann:preferredNamespacePrefix>
    <vann:preferredNamespaceUri>http://purl.org/ontology/places#</vann:preferredNamespaceUri>
    <msg0:license rdf:resource="http://creativecommons.org/licenses/by/1.0/"/>
    <rdfs:comment xml:lang="en">The Places Ontology is a simple lightweight ontology for describing places of geographic interest.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">地域本体是一个简单的轻量级本体，描述了与地理相关的地域。</rdfs:comment>
    <foaf:maker rdf:resource="http://purl.org/ontology/places/#michael"/>
    <foaf:maker rdf:resource="http://purl.org/ontology/places/#rob"/>
    <foaf:maker rdf:resource="http://purl.org/ontology/places/#tom"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#ArbitraryRegion">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An area that is useful to illustrate a particular point but does not have a recognised political or cultural identity.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一个区域，说明一个特定的点是非常有用的，但没有一个公认的政治或文化特征。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="es">Arbitraria Región</rdfs:label>
    <rdfs:label xml:lang="en">Arbitrary Region</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Région Arbitraire</rdfs:label>
    <rdfs:label xml:lang="de">Willkürliche Region</rdfs:label>
    <rdfs:label xml:lang="zh-CN">任意区域</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Bay">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An indentation to a shoreline, a body of water enclosed but with a large mouth to a wider body of water.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">缩进的海岸线，一个封闭的水体，但与更广泛的水体有接口。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="es">Bahía</rdfs:label>
    <rdfs:label xml:lang="de">Bay</rdfs:label>
    <rdfs:label xml:lang="en">Bay</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Bay</rdfs:label>
    <rdfs:label xml:lang="zh-CN">湾</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Beach">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">The shore of a body of water, usually sandy or pebbly.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">水体的岸边，通常是沙地或多卵石的。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="de">Beach</rdfs:label>
    <rdfs:label xml:lang="en">Beach</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Beach</rdfs:label>
    <rdfs:label xml:lang="es">Playa</rdfs:label>
    <rdfs:label xml:lang="zh-CN">海滩</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Borough">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">An administrative region of governance.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">管治的一个行政区域。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="fr-fr">Arrondissement</rdfs:label>
    <rdfs:label xml:lang="de">Bezirk</rdfs:label>
    <rdfs:label xml:lang="en">Borough</rdfs:label>
    <rdfs:label xml:lang="es">Municipio</rdfs:label>
    <rdfs:label xml:lang="zh-CN">行政区</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Boundary">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A Boundary resource specifies a sequence of points which, if joined, give the boundary of a geographical area. The sequence may be a serious of resources, each with a latitude and longitude, or a series of points using the datatype place:latlong. If the final point is the same as the first then the boundary is considered to be a closed area, whereas if the first and last points differ the boundary is considered to be a border line.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">指定了一个点序列的边界资源，这些点给出了一个地理区域的边界。这个序列可能是一系列资源，每个都有纬度和经度，或者是使用了数据类型place:latlong的一系列点。如果终点和起点相同，则所述边界被认为是一个封闭的区域，否则被认为是一个边界线。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="en">Boundary</rdfs:label>
    <rdfs:label xml:lang="zh-CN">边界</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Seq"/>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Cape">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A strip of land projecting into a body of water</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">伸入水体的狭长地带。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="es">Cabo</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Cap</rdfs:label>
    <rdfs:label xml:lang="de">Cape</rdfs:label>
    <rdfs:label xml:lang="en">Cape</rdfs:label>
    <rdfs:label xml:lang="zh-CN">海角</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#City">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A large settlement, often with some form of formal political recognition.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一个大的聚居地，往往具有某种形式的正规的政治认同。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="de">City</rdfs:label>
    <rdfs:label xml:lang="en">City</rdfs:label>
    <rdfs:label xml:lang="fr-fr">City</rdfs:label>
    <rdfs:label xml:lang="es">Ciudad</rdfs:label>
    <rdfs:label xml:lang="zh-CN">市</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/City"/>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/City"/>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Cliff">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A steep high face of rock.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">高而陡峭的岩石。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="es">Acantilado</rdfs:label>
    <rdfs:label xml:lang="de">Cliff</rdfs:label>
    <rdfs:label xml:lang="en">Cliff</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Cliff</rdfs:label>
    <rdfs:label xml:lang="zh-CN">悬崖</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Continent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A large landmass recognised by convention as a continent.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一个大的陆块，按惯例被公认为大陆。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="en">Continent</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Continent</rdfs:label>
    <rdfs:label xml:lang="es">Continente</rdfs:label>
    <rdfs:label xml:lang="de">Kontinent</rdfs:label>
    <rdfs:label xml:lang="zh-CN">大陆</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Continent"/>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Continent"/>
    <owl:equivalentClass rdf:resource="http://www.telegraphis.net/ontology/geography/geography#Continent"/>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Country">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A region considered to be the territory of a recognised political state.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一个地区，被认为是公认的的政治状态的领土。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="en">Country</rdfs:label>
    <rdfs:label xml:lang="de">Land</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Pays</rdfs:label>
    <rdfs:label xml:lang="es">País</rdfs:label>
    <rdfs:label xml:lang="zh-CN">国家</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Country"/>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Country"/>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#County">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A county is a land area of local governance within a country. A county may have cities and towns within its area.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">在一个国家内，一个县是地方治理的陆地区域，可能包含城镇。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="fr-fr">Comté</rdfs:label>
    <rdfs:label xml:lang="es">Condado</rdfs:label>
    <rdfs:label xml:lang="de">County</rdfs:label>
    <rdfs:label xml:lang="en">County</rdfs:label>
    <rdfs:label xml:lang="zh-CN">县</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/County"/>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Desert">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A desert is a landscape or region that receives an extremely low amount of precipitation, less than enough to support growth of most plants.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">沙漠是降水量非常低的区域，低到不足以支持大多数植物的生长。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="de">Desert</rdfs:label>
    <rdfs:label xml:lang="en">Desert</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Desert</rdfs:label>
    <rdfs:label xml:lang="es">Desierto</rdfs:label>
    <rdfs:label xml:lang="zh-CN">沙漠</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Desert"/>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#District">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A region of governance.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">管治的区域。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="de">Bezirk</rdfs:label>
    <rdfs:label xml:lang="en">District</rdfs:label>
    <rdfs:label xml:lang="fr-fr">District</rdfs:label>
    <rdfs:label xml:lang="es">Distrito</rdfs:label>
    <rdfs:label xml:lang="zh-CN">区</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Estuary">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">The widened area of a river as it opens to meet the sea or ocean.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一条河接触海或洋的增宽区域。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="fr-fr">Estuaire</rdfs:label>
    <rdfs:label xml:lang="es">Estuario</rdfs:label>
    <rdfs:label xml:lang="en">Estuary</rdfs:label>
    <rdfs:label xml:lang="de">Mündung</rdfs:label>
    <rdfs:label xml:lang="zh-CN">河口</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Glacier">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A slow-moving mass of ice.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">缓慢移动的大冰块。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="es">Glaciar</rdfs:label>
    <rdfs:label xml:lang="en">Glacier</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Glacier</rdfs:label>
    <rdfs:label xml:lang="de">Gletscher</rdfs:label>
    <rdfs:label xml:lang="zh-CN">冰川</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Gulf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">Part of a sea or ocean that is partially enclosed by land. Like a bay but larger.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">海或洋的一部分，部分被陆地包围起来。像湾，但更大。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="fr-fr">Golfe</rdfs:label>
    <rdfs:label xml:lang="es">Golfo</rdfs:label>
    <rdfs:label xml:lang="de">Gulf</rdfs:label>
    <rdfs:label xml:lang="en">Gulf</rdfs:label>
    <rdfs:label xml:lang="zh-CN">海湾</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Hamlet">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A Hamlet is a small human settlement consisting of just a few dwellings.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">村庄是一个小的只有几个住宅的人类聚居地。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="es">Aldea</rdfs:label>
    <rdfs:label xml:lang="en">Hamlet</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Hamlet</rdfs:label>
    <rdfs:label xml:lang="de">Weiler</rdfs:label>
    <rdfs:label xml:lang="zh-CN">村庄</rdfs:label>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Hill">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A hill is a landform that extends above the surrounding terrain.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">山丘是相比于周围区域向上延伸的地形。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="de">Hill</rdfs:label>
    <rdfs:label xml:lang="en">Hill</rdfs:label>
    <rdfs:label xml:lang="es">Hill</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Hill</rdfs:label>
    <rdfs:label xml:lang="zh-CN">山丘</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Hill"/>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Island">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A landmass surrounded entirely by water.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一个完全由水包围的陆块。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
    <rdfs:label xml:lang="es">Isla</rdfs:label>
    <rdfs:label xml:lang="de">Island</rdfs:label>
    <rdfs:label xml:lang="en">Island</rdfs:label>
    <rdfs:label xml:lang="fr-fr">Island</rdfs:label>
    <rdfs:label xml:lang="zh-CN">岛</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Island"/>
    <status:term_status>testing</status:term_status>
  </rdf:Description>

  <rdf:Description rdf:about="http://purl.org/ontology/places#Isthmus">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A narrow strip of land, bounded by water on two sides, and connecting two larger landmasses.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一个狭长的土地，两面临水界，并连接两个较大的陆地。</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/places#"/>
