      <owl:Class rdf:about="http://www.w3.org/2000/10/swap/pim/contact#Person">
        <rdfs:label xml:lang="en-US">Person</rdfs:label>
        <rdfs:label xml:lang="zh-CN">人物</rdfs:label>
        <!--<rdfs:comment xml:lang="en-US"></rdfs:comment>
        <rdfs:comment xml:lang="zh-CN"></rdfs:comment>-->
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing">
