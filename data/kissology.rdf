<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:kiss="http://www.gnowsis.org/ont/kissology#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:dc="http://purl.org/dc/elements/1.1/">
  <owl:Ontology rdf:about="http://www.gnowsis.org/ont/kissology" dc:date="$Date: 2003/09/21 15:21:35 $">
    <dc:title xml:lang="en-US">Kissology</dc:title>
    <dc:title xml:lang="zh-CN">吻之本体</dc:title>
    <dc:description xml:lang="en-US">Who kissed whom.</dc:description>
    <dc:description xml:lang="zh-CN">谁吻了谁。</dc:description>
    <owl:imports rdf:resource="http://www.w3.org/2000/01/rdf-schema"></owl:imports>
    <owl:imports rdf:resource="http://www.w3.org/2002/07/owl"></owl:imports>
  </owl:Ontology>
  <rdf:Property rdf:about="http://www.gnowsis.org/ont/kissology#kissed" rdfs:label="kissed">
    <rdfs:label xml:lang="en-US">kissed</rdfs:label>
    <rdfs:label xml:lang="zh-CN">吻了</rdfs:label>
    <rdfs:comment xml:lang="en-US">A Person had kissed another Person. More detail: Kissing in this property means to touch with the lips or press the lips against someone's mouth or other body part as an expression of love.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一个人吻了另一个人。更多的细节：在此属性中的接吻意味着对别人的嘴或身体的其他部分，用嘴唇接触或按嘴唇，以此作为一种爱的表达。</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person" />
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person" />
    <!-- hm, this is still under discussion: do I have to foaf:know people I kiss? 
	<rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/knows" />
	-->
    <rdfs:isDefinedBy rdf:resource="http://www.gnowsis.org/ont/kissology" />
