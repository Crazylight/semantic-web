    <rdfs:label>Object</rdfs:label>
    <rdfs:comment>Object is a subclass of SyntacticArgument representing the Object of a given Subcategorization Frame.</rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.lexinfo.net/lexinfo#PObject">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>PObject</rdfs:label>
    <rdfs:comment>PObject is a subclass of SyntacticArgument representing the Prepositional Object of a given Subcategorization Frame.</rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.lexinfo.net/lexinfo#Phrase">
