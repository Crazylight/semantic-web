    <!-- http://www.patexpert.org/ontologies/pso.owl#A1Document -->

    <owl:Class rdf:about="&pso;A1Document">
        <rdfs:subClassOf rdf:resource="&pso;EPOPatentKindDocument"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pso;hasFrontPageSection"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pso;hasSearchReportSection"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pso;hasClaimsSection"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pso;hasDescriptionSection"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pso;hasDrawingsSection"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string">A1Document is European patent application published with International search report. It consists of the following parts: a standardized front page, the description of the invention, the claims, the drawings and the international search report.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.patexpert.org/ontologies/pso.owl#A2Document -->

    <owl:Class rdf:about="&pso;A2Document">
        <rdfs:subClassOf rdf:resource="&pso;EPOPatentKindDocument"/>
