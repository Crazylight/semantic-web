            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string">A2Document is European patent application published without European search report. It consists of the following parts: a standardized front page, the description of the invention, the claims and the drawings.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.patexpert.org/ontologies/pso.owl#A3Document -->

    <owl:Class rdf:about="&pso;A3Document">
        <rdfs:subClassOf rdf:resource="&pso;EPOPatentKindDocument"/>
